<?xml version='1.0' encoding='utf-8'?>
<widget id="br.edu.eteczl.camera" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Proj2 Camera</name>
    <description>
        Simple Camera Project for Cordova Class Rooms
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        wagnerdocri@gmail.com
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
